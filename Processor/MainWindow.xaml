<Window x:Class="Processor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Processor"
        mc:Ignorable="d"
        Title="ProC#essor" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="ProcessGrid"  ColumnWidth="*" HorizontalAlignment="Left" Height="311" Margin="48,69,0,0" VerticalAlignment="Top" Width="680" IsReadOnly="True" SelectedCellsChanged="ProcessGrid_SelectedCellsChanged" Background="{x:Null}" />
        <CheckBox Content="Always on top" HorizontalAlignment="Left" Margin="19,19,0,0" VerticalAlignment="Top" Checked="CheckBox_Checked" Unchecked="CheckBox_Unchecked"/>

    </Grid>
</Window>
